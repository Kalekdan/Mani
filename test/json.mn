use("json");
import("maps");
use("files");
let f = fopen("test.json");
say f;
let conts = fread(f);
let jsonContents = json_parse(conts);
say jsonContents;
let m = Map();
m.copy(jsonContents);
say m;
say m.get("name");
m.update("age", 12);
say m;
m.add("lol", "me");
say m;
let final = json_encode(m.raw());
say final;
let newF = fopen("test_out.txt");
say fwrite(newF, final, "w");