# "lists";

class Patient {
    Patient(id, first, last, tLevel, bType) {
        this.firstName = first;
        this.lastName = last;
        this.tLevel = tLevel;
        this.bType = bType;
        this.id = id;
    }

    getFirstName() { return this.firstName; }
    getLastName() { return this.lastName; }
    getLevel() { return this.lastName; }
    getBloodType() { return this.bType; }
    getTreatment() { return this.tLevel; }
    getID() { return this.id; }

    show() {
        say TAB + "[Patient] Name: " + this.firstname + " " + this.lastName + ", Blood Type: " + this.bType + ", Treatment Level: " + this.tLevel;
    }
}